{
  "name": "ManagedSet",
  "version": "0.0.0-dev",
  "description": "Kapitan: component to manage kubernetes resources",
  "main": "src/index.js",
  "keywords": [
    "kubernetes",
    "razee",
    "razeedash",
    "manage"
  ],
  "scripts": {
    "start": "node src/index.js",
    "test": "nyc --reporter=html --all --reporter=text mocha ",
    "test:debug": "mocha --inspect-brk",
    "check-coverage": "nyc check-coverage --statements 0 --branches 0 --functions 0 --lines 0",
    "lint": "npx npm-run-all eslint yaml-lint markdownlint jsonlint shellcheck",
    "eslint": "npx eslint src/",
    "yaml-lint": "npx yamllint .travis.yml kubernetes/ManagedSet/resource.yaml",
    "markdownlint": "npx markdownlint README.md docs/",
    "dockerlint": "npx dockerlint Dockerfile",
    "jsonlint": "npx jsonlint --quiet build/viewTemplate.json",
    "shellcheck": "npx shellcheck build/process-template.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:razee-io/ManagedSet.git"
  },
  "contributors": [
    {
      "name": "Alex Lewitt"
    },
    {
      "name": "Adam King"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@razee/kubernetes-util": "^0.1.6",
    "@razee/razeedeploy-core": "^0.10.1",
    "bunyan": "^1.8.14",
    "object-path": "^0.11.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "dockerlint": "^0.3.9",
    "eslint": "^7.14.0",
    "jsonlint": "^1.6.3",
    "markdownlint-cli": "^0.25.0",
    "mocha": "^8.2.1",
    "nock": "^13.0.5",
    "npm-check-updates": "^10.2.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "shellcheck": "^0.4.4",
    "sinon": "^9.2.1",
    "yaml-lint": "^1.2.4"
  }
}
